{
  "name": "@mixspa/core",
  "version": "0.5.1",
  "description": "This is a library for create a micro-frontend app",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "babel": "babel src --out-dir lib --ignore 'src/**/*.spec.js'",
    "babel:watch": "watch 'yarn babel' ./src",
    "build": "yarn clean && yarn babel",
    "lint": "eslint src",
    "test": "jest --coverage --passWithNoTests --config config/jest.config.js",
    "test:watch": "watch 'yarn test' ./src"
  },
  "dependencies": { },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^5.15.0",
    "eslint-plugin-jest": "^22.13.6",
    "jest": "^24.8.0",
    "watch": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixspa/mixspa-core.git"
  },
  "keywords": [
    "lib",
    "node",
    "babel",
    "jest",
    "eslint",
    "security",
    "circleci"
  ],
  "author": "Cao Xiaoqi<xqcao@xqcao.com>",
  "contributors": [
    "Xiaoqi Cao"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixspa/mixspa-core/issues"
  },
  "homepage": "https://github.com/mixspa/mixspa-core#readme"
}
